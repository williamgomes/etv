<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EssentialTv\EtvBundle\Entity\ConfigSettings" table="config_settings" repository-class="EssentialTv\EtvBundle\Entity\ConfigSettingsRepository">
    <id name="csId" type="integer" column="CS_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="csOption" type="string" column="CS_option" length="100" nullable="false"/>
    <field name="csValue" type="string" column="CS_value" length="500" nullable="false"/>
    <field name="csUpdateDate" type="datetime" column="CS_update_date" nullable="false"/>
    <field name="csUpdatedBy" type="string" column="CS_updated_by" length="200" nullable="false"/>
    <field name="csAutoLoad" type="string" column="CS_auto_load" nullable="false"/>
  </entity>
</doctrine-mapping>
